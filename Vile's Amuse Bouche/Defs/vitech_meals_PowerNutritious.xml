<Defs>
	<ThingDef ParentName="SK_MealBase">
		<defName>PastaPrimavera</defName>
		<label>pasta primavera</label>
		<description>Nutritious Food 60% (.85 Nutr, Fine)  &#xA; Origin: Nova Scotia, CA &#xA; A pasta dish with a variety of fresh vegetables.</description>
		<graphicData>
			<texPath>Things/Item/Meal/PastaPrimavera</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<DeteriorationRate>10</DeteriorationRate>
			<MarketValue>40</MarketValue>
			<WorkToMake>720</WorkToMake>
			<Bulk>1.50</Bulk>
			<Mass>1.20</Mass>
			<Nutrition>0.75</Nutrition>
		</statBases>
		<ingestible>
			<tasteThought>AteFineMeal</tasteThought>
			<preferability>MealFine</preferability>
			<!--<joy>0.0</joy>
			<joyKind>Gluttonous</joyKind>-->
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
			<outcomeDoers>
				<li Class="IngestionOutcomeDoer_GiveHediff">
					<hediffDef>HedNutritiousFood</hediffDef>
					<severity>0.6</severity>
				</li>
			</outcomeDoers>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>5</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>
