<!-- BRAIN -->
<Defs>
	<ThingDef ParentName="SK_MealBase">
		<defName>Ramen</defName>
		<label>ramen</label>
		<description>Brain Food 60% (.8 Nutr, Fine)  &#xA; Origin: Japan &#xA; A noodle soup with a variety of toppings such as meat, vegetable, egg, or fermented bamboo shoots.</description>
		<graphicData>
			<texPath>Things/Item/Meal/Ramen</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<DeteriorationRate>10</DeteriorationRate>
			<MarketValue>40</MarketValue>
			<WorkToMake>900</WorkToMake>
			<Bulk>1.50</Bulk>
			<Mass>1.20</Mass>
			<Nutrition>0.8</Nutrition>
		</statBases>
		<ingestible>
			<tasteThought>AteFineMeal</tasteThought>
			<preferability>MealFine</preferability>
			<!--<joy>0.0</joy>
			<joyKind>Gluttonous</joyKind>-->
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
			<outcomeDoers>
				<li Class="IngestionOutcomeDoer_GiveHediff">
					<hediffDef>HedBrainFood</hediffDef>
					<severity>0.6</severity>
				</li>
			</outcomeDoers>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>5</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>
	
