<?xml version="1.0" encoding="UTF-8"?>
<!-- COMFORT FOOD-->
<Defs>
	<ThingDef ParentName="SK_MealBase">
		<defName>Soup</defName>
		<label>soup</label>
		<description>.8 Nutr &#xA; Pro tip: If you water down your food, it will last longer</description>
		<graphicData>
			<texPath>Things/Item/Meal/Soup</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<DeteriorationRate>10</DeteriorationRate>
			<MarketValue>20</MarketValue>
			<WorkToMake>900</WorkToMake>
			<Bulk>1.50</Bulk>
			<Mass>1.20</Mass>
			<Nutrition>0.8</Nutrition>
		</statBases>
		<ingestible>
			<!--<tasteThought>AteFineMeal</tasteThought>-->
			<preferability>MealSimple</preferability>
			<!--<joy>0.0</joy>
			<joyKind>Gluttonous</joyKind>-->
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>5</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>


	<ThingDef ParentName="SK_MealBase">
		<defName>Gruel</defName>
		<label>gruel</label>
		<description>.7 Nutr &#xA; Watered-down porridge. Gross, unless you're a peasant.</description>
		<graphicData>
			<texPath>Things/Item/Meal/Gruel</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<DeteriorationRate>10</DeteriorationRate>
			<MarketValue>10</MarketValue>
			<WorkToMake>400</WorkToMake>
			<Bulk>1.50</Bulk>
			<Mass>1.20</Mass>
			<Nutrition>0.7</Nutrition>
		</statBases>
		<ingestible>
			<tasteThought>AteBlandMeal</tasteThought>
			<preferability>MealAwful</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>5</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>


</Defs>