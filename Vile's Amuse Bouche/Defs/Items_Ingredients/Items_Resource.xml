<?xml version="1.0" encoding="utf-8" ?>
<Defs>


	<ThingDef ParentName="CropsBase">
		<defName>Pasta</defName>
		<label>pasta</label>
		<description>Dry pasta made from flour.</description>
		<graphicData>
			<texPath>Things/Item/pasta</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>10</MarketValue>
			<Nutrition>0.05</Nutrition>
		</statBases>
		<ingestible>
			<foodType>Processed</foodType>
			<preferability>DesperateOnly</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>
		<thingCategories>
			<li>CookingSupplies</li>
		</thingCategories>
		<tickerType>Rare</tickerType>   
		<stuffProps>
			<categories>
				<li>Stuff</li>
			</categories>
		</stuffProps>   
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>90</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>
	
	<ThingDef ParentName="SK_PlantFoodRawBase">
