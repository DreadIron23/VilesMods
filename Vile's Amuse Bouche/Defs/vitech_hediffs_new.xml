<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<HediffDef>
		<defName>HedHauteCuisine</defName>
		<label>ate haute cousine</label>
		<description>Wow, what an impressive meal. A perfect balance of umami and richness.</description>
		<initialSeverity>1</initialSeverity>
		<hediffClass>HediffWithComps</hediffClass>
		<isBad>false</isBad>
		<comps>
			<li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>-0.5</severityPerDay>
			</li>
		</comps>
		<stages>
			<li>
				<minSeverity>0</minSeverity>
				<label>Well Fed: Haute Cuisine</label>
				<painFactor>0.75</painFactor>
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>0.2</offset>
					</li>
					<li>
						<capacity>Hearing</capacity>
						<offset>0.2</offset>
					</li>
				</capMods>
				<statOffsets>
					<Comfort>.1</Comfort>
					<MentalBreakThreshold>-.15</MentalBreakThreshold>
				</statOffsets>
				<!--<baseMoodEffect>10</baseMoodEffect>-->
			</li>
		</stages>
	</HediffDef>
		  
