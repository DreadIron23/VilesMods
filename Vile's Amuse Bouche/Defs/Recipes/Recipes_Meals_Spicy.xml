<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>MakeTacos</defName>
		<label>make tacos (x4 spicy)</label>
		<description>Spicy Food 60% (.6 Nutr.) &#xA; Origin: Mexico &#xA; Handheld corn tortilla filled with spicy meat or bean mix.</description>
		<jobString>Making tacos.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<workAmount>720</workAmount>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Cornmeal</li>
					</thingDefs>
				</filter>
				<count>0.4</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>RawPeppers</li>
					</thingDefs>
				</filter>
				<count>0.2</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
						<li>MeatSubRaw</li>
					</categories>
					<thingDefs>
						<li>Rawbean</li>
					</thingDefs>
				</filter>
				<count>.4</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>RawTomatoes</li>
					</thingDefs>
				</filter>
				<count>0.2</count>
			</li>
		</ingredients>
		<products>
			<Taco>4</Taco>
		</products>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
				<li>BasicPlantFoodRaw</li>
				<li>MeatSubRaw</li>
			</categories>
			<thingDefs>
				<li>RawPeppers</li>
				<li>RawTomatoes</li>
				<li>Cornmeal</li>
			</thingDefs>
			<specialFiltersToDisallow>
				<li>AllowPlantFood</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>
				<li>BasicPlantFoodRaw</li>
				<li>MeatSubRaw</li>
			</categories>
			<thingDefs>
				<li>RawPeppers</li>
				<li>RawTomatoes</li>
				<li>Cornmeal</li>
			</thingDefs>
			<disallowedThingDefs>
				<li>Meat_Megaspider</li>
				<li>Meat_Human</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>
		<skillRequirements>
			<Cooking>9</Cooking>
		</skillRequirements>
		<workSkill>Cooking</workSkill>
		<recipeUsers>
			<li>ElectricStove</li>
			<li>ElectricStove_Pro</li>
		</recipeUsers>
		<researchPrerequisite>Food_C1</researchPrerequisite>
	</RecipeDef>
	
	
