<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef>
		<defName>MakeFishandChips</defName>
		<label>fish and chips (x2 comfort)</label>
		<description>Comfort Food 60% (.85 Nutr, .2 Joy)  &#xA; Origin: UK &#xA; Beer-battered fish and potatoes.</description>
		<jobString>Making fish and chips.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<workAmount>720</workAmount>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<!--<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>-->
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RawPotatoes</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Meat_ThingDefFishSduiggles</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Patreonbrew</li>
						<li>Beer</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<products>
			<FishandChips>3</FishandChips>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>RawPotatoes</li>
				<li>Meat_ThingDefFishSduiggles</li>
				<li>Patreonbrew</li>
				<li>Beer</li>
			</thingDefs>
			<specialFiltersToDisallow>
				<li>AllowPlantFood</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>		
			<thingDefs>
				<li>RawPotatoes</li>
				<li>Meat_ThingDefFishSduiggles</li>
				<li>Patreonbrew</li>
				<li>Beer</li>
			</thingDefs>
			</defaultIngredientFilter>
		<skillRequirements>
			<Cooking>5</Cooking>
		</skillRequirements>
		<workSkill>Cooking</workSkill>
		<recipeUsers>
			<li>Fryer</li>
		</recipeUsers>
		<researchPrerequisite>Food_C6</researchPrerequisite>
	</RecipeDef>
	
