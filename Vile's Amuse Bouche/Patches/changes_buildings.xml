<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Core SK</li>
	</mods>
	<match Class="PatchOperationSequence">
			<operations>
        		
				
			<li Class="PatchOperationReplace">
            			<xpath>Defs/ThingDef[defName="ElectricStove_Pro"]/statBases/WorkTableWorkSpeedFactor</xpath>
            				<value>
								<WorkTableWorkSpeedFactor>1.2</WorkTableWorkSpeedFactor>
            				</value>
        	</li>
			
			<li Class="PatchOperationReplace">
            			<xpath>Defs/ThingDef[defName="TableOven"]/graphicData/drawSize</xpath>
            				<value>
								<drawSize>(6,6)</drawSize>
            				</value>
        	</li>
					
			</operations>
			</match>
    </Operation>	

	
</Patch>