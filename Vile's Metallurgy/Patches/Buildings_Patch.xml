		<Operation Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="Campfire"]/recipes</xpath>
				<value>
					<recipes></recipes>
				</value>
			</Operation>	
	
		<Operation Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="CoalPowerPlant"]/comps/li[@Class="SK.CompFueled_Properties"]/fuelFilter</xpath>
				<value>
				<fuelFilter>
