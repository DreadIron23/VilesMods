				<value>
				<fuelFilter>
					<thingDefs>
						<li>Chemfuel</li>
						<li>Kindling</li>
						<li>Charcoal</li>
						<li>Peat</li>
					</thingDefs>
					<categories>
						<li>Wooden</li>
					</categories>
				</fuelFilter>
				</value>
		</Operation>	
	

	
	
