<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Core SK</li>
	</mods>
	<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="TableSawmillElectric"]/researchPrerequisites</xpath> 
				<value>
					<researchPrerequisites>
						<li>Concrete_C1</li>
					</researchPrerequisites>
				</value>
			</li>
		</operations>
		</match>
    </Operation>	

	<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Vile's Amuse Bouche</li>
	</mods>
	<match Class="PatchOperationSequence">
		<operations>
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="Hearth" or defName="OpenHearth"]/comps/li[@Class="SK.CompFueled_Properties"]/fuelFilter</xpath> 
					<value>
							<fuelFilter>
								<thingDefs>
									<li>Coal</li>
									<li>Charcoal</li>
									<li>Peat</li>
									<li>Kindling</li>
								</thingDefs>
								<categories>
									<li>Wooden</li>
								</categories>
							</fuelFilter>
					</value>
				</li>
		</operations>
		</match>
    </Operation>	
	
	
	
</Patch>	

