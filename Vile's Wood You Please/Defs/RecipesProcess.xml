<Defs>

<RecipeDef Name="SeasonFirewoodBase" Abstract="True">
	<recipeUsers>
		<li>SeasoningRack</li>
	</recipeUsers>
	<temperatureSafe>
		<min>-273</min>
		<max>1000</max>
	</temperatureSafe>
	<temperatureIdeal>
		<min>20</min>
		<max>60</max>
	</temperatureIdeal>
	<sunFactor>
		<min>0.75</min>
		<max>1.35</max>
	</sunFactor>
	<rainFactor>
		<min>1</min>
		<max>0.5</max>
	</rainFactor>
	<speedBelowSafe>0.2</speedBelowSafe>
	<speedAboveSafe>1</speedAboveSafe>
	<graphicSuffix>_filled</graphicSuffix>
</RecipeDef>

<RecipeDef ParentName="SeasonFirewoodBase" Class="UniversalFermenterSK.RecipeDef_UF">
	<defName>SeasonFirewood_Cecropia</defName>
	<label>season cecropia</label>
	<description>Burns 60% more efficiently. Takes 30 days to season.</description>
