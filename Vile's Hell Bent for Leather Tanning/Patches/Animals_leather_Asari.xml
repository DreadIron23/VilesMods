<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
	<mods>
		<li>AsariRace</li>
	</mods>
	<match Class="PatchOperationSequence">
	<success>Always</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Asari"]/statBases/LeatherAmount</xpath>
				<value>
					<LeatherAmount>4</LeatherAmount>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Asari"]/race/leatherDef</xpath>
				<value>
					<leatherDef>Hide_HumanSkin</leatherDef>
				</value>
			</li>	
		</operations>
		</match>
    </Operation>	
</Patch>	