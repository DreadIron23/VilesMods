			<statFactors>
				<MaxHitPoints>1</MaxHitPoints>
				<Beauty>0.7</Beauty>
				<MoveSpeed>1</MoveSpeed>
				<MarketValue>0.75</MarketValue>
				<WorkToMake>1</WorkToMake>
				<WorkToBuild>1</WorkToBuild>
				<BedRestEffectiveness>1</BedRestEffectiveness>
				<ImmunityGainSpeedFactor>0.95</ImmunityGainSpeedFactor>
				<Mass>0.55</Mass>
				<Cleanliness>1</Cleanliness>
			</statFactors>
		</stuffProps>
	</ThingDef>
	
	<ThingDef ParentName="CropsBase">
		<defName>Leather_Scraps</defName>
		<label>leather strips</label>
		<description>Shredded pieces of leather which can be used to make bonded leather or as a component in certain recipes.</description>
		<graphicData>
			<texPath>Things/Item/Components/LeatherStrips</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<techLevel>Neolithic</techLevel>
		<statBases>
			<MarketValue>0.2</MarketValue>
			<DeteriorationRate>0</DeteriorationRate>
			<Flammability>0.5</Flammability>
			<SellPriceFactor>0.50</SellPriceFactor>
			<Mass>0.25</Mass>
			<Bulk>0.25</Bulk>
		</statBases>
		<useHitPoints>true</useHitPoints>
		<thingCategories>
			<li>Parts</li>
		</thingCategories>
		<stackLimit>500</stackLimit>
	</ThingDef>
	
	
	
	
