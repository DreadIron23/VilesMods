<Defs>

	<ThingDef ParentName="SK_LeatherBase">
		<defName>TannedFur</defName>
		<label>tanned fur</label>
		<description>A crudely-tanned animal fur. Warmer and comfier than buckskin, but it's not a very strong material.</description>
		<graphicData>
			<color>(147,105,72)</color>
		</graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<MarketValue>1.8</MarketValue>
			<StuffPower_Armor_Sharp>0.5</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Blunt>0.9</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Heat>0.08</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>32</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>-4</StuffPower_Insulation_Heat>
			<Mass>0.01</Mass>
			<Bulk>0.01</Bulk>
		</statBases>
		<stuffProps>
			<color>(147,105,72)</color>
			<commonality>0.01</commonality>
			<stuffAdjective>tanned fur</stuffAdjective>
			<statFactors>
				<MaxHitPoints>0.82</MaxHitPoints>
				<Beauty>1.4</Beauty>
				<MoveSpeed>1</MoveSpeed>
				<MarketValue>0.75</MarketValue>
				<WorkToMake>1.8</WorkToMake>
				<WorkToBuild>1.8</WorkToBuild>
				<BedRestEffectiveness>0.9</BedRestEffectiveness>
				<ImmunityGainSpeedFactor>0.8</ImmunityGainSpeedFactor>
				<Mass>0.7</Mass>
				<Cleanliness>0.5</Cleanliness>
			</statFactors>
		</stuffProps>
	</ThingDef>
	
	<ThingDef Name="LeatherBase_Upholstery" ParentName="SK_LeatherBase">
		<defName>Leather_UpholsteryBrown</defName>
		<label>upholstery leather (brown)</label>
		<description>Corrected-grain leather that is finished and embossed. It's attractive, sturdy, and somewhat comfortable, and great for furniture of all types, but makes for terrible clothing. Very easy to clean.</description>
		<graphicData>
			<color>(188,84,68)</color>
		</graphicData>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<MarketValue>4</MarketValue>
			<StuffPower_Armor_Sharp>0.8</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Blunt>0.6</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Heat>0.1</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>25</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>0</StuffPower_Insulation_Heat>
			<Mass>0.01</Mass>
			<Bulk>0.01</Bulk>
		</statBases>
		<stuffProps>
			<color>(188,84,68)</color>
			<commonality>0.0025</commonality>
			<stuffAdjective>upholstery leather</stuffAdjective>
			<statFactors>
				<MaxHitPoints>1.5</MaxHitPoints>
				<Beauty>3</Beauty>
				<MoveSpeed>1</MoveSpeed>
				<MarketValue>0.8</MarketValue>
				<WorkToMake>1</WorkToMake>
				<WorkToBuild>1.5</WorkToBuild>
				<BedRestEffectiveness>1.1</BedRestEffectiveness>
				<ImmunityGainSpeedFactor>1</ImmunityGainSpeedFactor>
				<Mass>0.7</Mass>
				<Cleanliness>1.5</Cleanliness>
			</statFactors>
		</stuffProps>
	</ThingDef>
