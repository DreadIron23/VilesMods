<?xml version="1.0" encoding="UTF-8"?>
<!-- COMFORT FOOD-->
<Defs>
	<ThingDef ParentName="SK_MealBase">
		<defName>Poutine</defName>
		<label>poutine</label>
		<description>Comfort Food 60% (.6 Nutr, .1 Joy, Fine)  &#xA; Origin: Quebec, CA &#xA; Fried potatoes with gravy and cheese curds.</description>
		<graphicData>
			<texPath>Things/Item/Meal/Poutine</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<DeteriorationRate>10</DeteriorationRate>
			<MarketValue>20</MarketValue>
			<WorkToMake>900</WorkToMake>
			<Bulk>1.50</Bulk>
			<Mass>1.20</Mass>
			<Nutrition>0.6</Nutrition>
		</statBases>
		<ingestible>
			<tasteThought>AteFineMeal</tasteThought>
			<preferability>MealFine</preferability>
			<!--<joy>0.0</joy>
			<joyKind>Gluttonous</joyKind>-->
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
			<outcomeDoers>
				<li Class="IngestionOutcomeDoer_GiveHediff">
					<hediffDef>HedComfortFood</hediffDef>
					<severity>0.6</severity>
				</li>
			</outcomeDoers>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>5</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>
	
