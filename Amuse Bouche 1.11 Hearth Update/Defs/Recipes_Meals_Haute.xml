<Defs>

	<RecipeDef>
		<defName>MakeDuckalorange</defName>
		<label>Duck a l'orange (x4 Haute Cuisine)</label>
		<description>Haute Cousine 60% (1.0 Nutr, .3 Joy, Lavish, Beauty +16)  &#xA; Origin: France &#xA; Roast duck in an orange sauce. Five star.</description>
		<jobString>Making duck a l'orange.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<workAmount>2400</workAmount>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<!--<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>-->
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Corpse_Duck</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Raworange</li> 	
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>ExtraPlantFoodRaw</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>LuxuryStuffs</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<products>
			<Duckalorange>1</Duckalorange>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Corpse_Duck</li>
				<li>Raworange</li>
			</thingDefs>
			<categories>
				<li>ExtraPlantFoodRaw</li>
				<li>LuxuryStuffs</li>
			</categories>
			<specialFiltersToDisallow>
				<li>AllowPlantFood</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>		
			<thingDefs>
				<li>Corpse_Duck</li>
				<li>Raworange</li>
