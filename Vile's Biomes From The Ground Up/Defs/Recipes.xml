<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedPricklyPear</defName>
    <label>extract prickly pear seeds</label>
    <description>Extract seeds.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawPricklyPear</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawPricklyPear</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_PricklyPear>3</Seed_PricklyPear>
    </products>
    <researchPrerequisite>Cocoa</researchPrerequisite>
  </RecipeDef>
  
