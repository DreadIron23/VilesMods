<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Core SK</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[DefName="HeavyPowerConduit"]</xpath>
                    <value>                  
                        <CarbonAlloy>2</CarbonAlloy> <!-- nanocomposite -->
                    </value>
                </li>
                
                 <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="HeavyPowerConduit"]/costList</xpath>
                    <value>      
                        <costList>
                            <Wire>1</Wire>
                            <PVCPlastic>1</PVCPlastic>
                          </costList>
                    </value>
                </li>
                
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="AdvancedSolarGenerator"]/costList</xpath>
                    <value>                  
                       <costList>
                            <Polycarbonate>45</Polycarbonate>
                            <Glass>45</Glass>
                            <ReinforcedConcrete>12</ReinforcedConcrete>
                            <AdvMechanism>5</AdvMechanism>
                            <Electronics>5</Electronics>
                        </costList>
                    </value>
                </li>
                
                <!-- WIND TURBINE -->
                 <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="WindTurbine"]/costList</xpath>
                    <value>                  
                       <costList>
                            <ComponentIndustrial>8</ComponentIndustrial>
                            <ElectronicComponents>2</ElectronicComponents>
                            <Mechanism>3</Mechanism>
                             <ReinforcedConcrete>8</ReinforcedConcrete>
                           <Wire>10</Wire>
                        </costList>
                    </value>
                </li>              
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="WindTurbine"]/stuffCategories</xpath>
                    <value>                  
                      <stuffCategories>
                            <li>LightMaterial</li>
                        </stuffCategories>
                    </value>
                </li>               
                 <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="WindTurbine"]/costStuffCount</xpath>
                    <value>                  
                        <costStuffCount>110</costStuffCount>
                    </value>
                </li>
                 <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="WindTurbine"]/statBases/MaxHitPoints</xpath>
                    <value>                  
                       <MaxHitPoints>325</MaxHitPoints>
                    </value>
                </li>
                
                  <!-- ADV WIND TURBINE -->
                 <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="AdvancedWindTurbine"]/costList</xpath>
                    <value>                  
                         <costList>
                            <Fiberglass>60</Fiberglass>
                             <PVCPlastic>8</PVCPlastic>
                             <Compaste>8</Compaste>
                            <ReinforcedConcrete>16</ReinforcedConcrete>
                            <AdvMechanism>8</AdvMechanism>
                            <Electronics>4</Electronics>
                             <Wire>16</Wire>
                        </costList>
                    </value>
                </li>              

                
                 <!-- fuel PLANT -->
                 <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="ChemfuelPoweredGenerator"]/costList</xpath>
                    <value>                  
                          <costList>
                            <Paraffins>10</Paraffins>
                            <CarbonSteel>50</CarbonSteel>
                            <AnodizedAluminiumBlue>30</AnodizedAluminiumBlue>
                            <ComponentIndustrial>10</ComponentIndustrial>
                            <Mechanism>6</Mechanism>
                        </costList>
                    </value>
                </li>              
 
                 <!--chemfuel PLANT -->
                 <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="FuelPowerplant"]/costList</xpath>
                    <value>                  
                          <costList>
                            <Paraffins>10</Paraffins>
                            <SteelBar>35</SteelBar>
                             <AnodizedAluminiumBlue>20</AnodizedAluminiumBlue>
                            <ComponentIndustrial>8</ComponentIndustrial>
                            <Mechanism>5</Mechanism>
                        </costList>
                    </value>
                </li>      
                
                 <!--geothermal genny -->
                   <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="GeothermalGenerator"]/stuffCategories</xpath>
                    <value>                  
                      <stuffCategories>
                            <li>CorrosionResistant</li>
                        </stuffCategories>
                    </value>
                </li>    
                 <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="GeothermalGenerator"]/costList</xpath>
                    <value>                  
                          <costList>     
                              <CupronickelAlloy>20</CupronickelAlloy>
                            <ReinforcedConcrete>60</ReinforcedConcrete>
                            <Polycarbonate>40</Polycarbonate>
                            <Electronics>12</Electronics>
                            <Mechanism>8</Mechanism>
                        </costList>
                    </value>
                </li>      
                
                <!-- Thermoelectric -->
                   <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="ThermoelectricGenerator"]/costList</xpath>
                    <value>                  
                        <costList>
                            <Aerographene>70</Aerographene>
                            <Rubber>60</Rubber>
