                            <Microchips>4</Microchips>
                        </costList>
                    </value>
                </li>    
                
                   <!-- GAUSS WIND TURBINE -->
                 <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="GauseWindTurbine"]/costList</xpath>
                    <value>                  
                       <costList>
                            <Compaste>50</Compaste>
                           <CarbonAlloy>140</CarbonAlloy>
                            <ReinforcedConcrete>80</ReinforcedConcrete>
                            <AdvMechanism>5</AdvMechanism>
                            <Microchips>2</Microchips>
                           <Wire>10</Wire>
                        </costList>
                    </value>
                </li>      
                
                   <!-- Tesla Genny -->
                 <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="TeslaLigthningPower"]/costList</xpath>
                    <value>                  
                      <costList>
                        <CopperBar>50</CopperBar>
                        <Glass>200</Glass>
                        <HMFibers>200</HMFibers>
                        <ReinforcedConcrete>100</ReinforcedConcrete>
                        <Microchips>6</Microchips>
                    </costList>
                    </value>
                </li>     
                
                <!-- Binary Cycle Geothermal Plant -->
                      <!-- plasma geothermal -->
                 <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="PlasmaGeothermalPlant"]/costList</xpath>
                    <value>                  
                         <costList>
                            <turbineBlade>20</turbineBlade>
                             <AerMet>30</AerMet>
                                <ReinforcedConcrete>100</ReinforcedConcrete>
                              <AdvMechanism>5</AdvMechanism>
                            <Microchips>5</Microchips>
                        </costList>
                    </value>
                </li>               
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[defName="PlasmaGeothermalPlant"]/comps</xpath>
                    <value>                  
                       <comps>
                            <li Class="CompProperties_Flickable"/>
                            <li Class="CompProperties_Breakdownable"/>
                            <li Class="CompProperties_Power">
                                <compClass>SK.CompPowerPlantFueled</compClass>
                                <basePowerConsumption>-20000</basePowerConsumption>
                                <transmitsPower>true</transmitsPower>
                            </li>
                            <li Class="SK.CompFueled_Properties">
                                <canAutoBurn>true</canAutoBurn>
                                <operatingTemp>3000</operatingTemp>
                                <drawfirefuel>false</drawfirefuel>
                                <smokeDrawOffset>(0,0,1.5)</smokeDrawOffset>
                                <smokesize>2.0</smokesize>
                                <fuelCapacity>25</fuelCapacity>
                                <burnFuelMultiplier>0.25</burnFuelMultiplier>
                                <smokeEnabled>false</smokeEnabled>
                                <fuelFilter>
                                    <thingDefs>
                                        <li>SyntheticAmmonia</li>
                                    </thingDefs>
                                </fuelFilter>
                            </li>
                            <li Class="CompProperties_Glower">
                                <glowRadius>5</glowRadius>
                                <glowColor>(0,255,246,1)</glowColor>
                            </li>
                            <li Class="SK.CompProperties_HeatPusherAdvanced">
                                <compClass>SK.CompHeatPusherAdvanced</compClass>
                                <heatPerSecond>40</heatPerSecond>
                                <heatPushMaxTemperature>75</heatPushMaxTemperature>
                            </li>
                            <li Class="CompProperties_Explosive">
                                <explosiveRadius>7</explosiveRadius>
                                <explosiveDamageType>Flame</explosiveDamageType>
                            </li>
                            <li Class="SK.CompProperties_RestrictedPlacement">
                                <compClass>SK.CompRestrictedPlacement</compClass>
                                <restrictedThing>
                                    <li>SteamGeyser</li>
                                </restrictedThing>
                            </li>
                        </comps>
                    </value>
                </li>  
                <!-- Plasma generator -->
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="PlasmaGen"]/costList</xpath>
                    <value>                  
                        <costList>
                            <Molybdenum>20</Molybdenum>
                            <PobediteAlloy>20</PobediteAlloy>
                            <Fiberglass>50</Fiberglass>
                            <ReinforcedConcrete>25</ReinforcedConcrete>
                            <Microchips>2</Microchips>
                        </costList>    
                    </value>
                </li>    
                
                
                 <!-- ion xenon -->
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="XenonIonTurbine"]/costList</xpath>
                    <value>                  
                        <costList>
                            <turbineBlade>40</turbineBlade>
                            <Fiberglass>60</Fiberglass>
                            <ReinforcedConcrete>60</ReinforcedConcrete>
                            <BiosyntheticMaterial>60</BiosyntheticMaterial>
                            <AdvMechanism>12</AdvMechanism>
                            <Microchips>5</Microchips>
                        </costList>
                    </value>
                </li>    
                
                
             <!-- antimatter -->
                <li Class="PatchOperationReplace">
                    <xpath>Defs/ThingDef[DefName="AntimatterReactor"]/costList</xpath>
                    <value>                  
                       <costList>
                            <turbineBlade>100</turbineBlade>
                            <Fiberglass>20</Fiberglass>
                            <CarbonAlloy>40</CarbonAlloy>
                            <ReinforcedConcrete>200</ReinforcedConcrete>
                            <MagneticMaterial>65</MagneticMaterial>
                            <AdvMechanism>15</AdvMechanism>
                            <Microchips>5</Microchips>
                        </costList>
                    </value>
                </li>    
                
			</operations>
		</match>
    </Operation>	
</Patch>