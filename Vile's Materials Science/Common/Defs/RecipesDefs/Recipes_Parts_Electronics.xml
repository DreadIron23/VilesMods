<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef>
		<defName>MakeSilicon_Electric</defName>
		<label>Make semiconductor material x5</label>
		<description>Doesn't quite conduct, doesn't quite not. Critical component for electronics. Typically made from silicon.</description>
		<jobString>Making semiconductor material.</jobString>
		<workAmount>800</workAmount>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Smelt</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>GlassBatch</li>
					</thingDefs>
				</filter>
				<count>15</count>
			</li>
		</ingredients>
		<products>
			<Silicon>5</Silicon>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>GlassBatch</li>
			</thingDefs>
		</fixedIngredientFilter>
        <recipeUsers>
			<li>GlassworkTable</li>
		</recipeUsers>
		<skillRequirements>
			<Crafting>5</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
        <researchPrerequisite>MicroelectronicsBasics</researchPrerequisite>
	</RecipeDef>
	
	<RecipeDef>
		<defName>MakeElectronicComponents</defName>
		<label>Assemble transistors x5</label>
		<description>Transistors are an important basic component of electronics.</description>
		<jobString>Assembling transistors.</jobString>
		<workAmount>2500</workAmount>
		<workSpeedStat>ElectronicCraftingSpeed</workSpeedStat>
		<effectWorking>Smith</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<recipeUsers>
			<li>TableElectronics</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>INSBar</li>
					</categories>
					<thingDefs>
						<li>Glass</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Silicon</li>
					</thingDefs>
				</filter>
				<count>30</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SilverBar</li>
						<li>AluminiumBar</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Solder</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Silicon</li>
				<li>AluminiumBar</li>
				<li>Solder</li>
				<li>SilverBar</li>
				<li>Glass</li>
			</thingDefs>
			<categories>
				<li>INSBar</li>
			</categories>
		</fixedIngredientFilter>
		<products>
			<ElectronicComponents>5</ElectronicComponents>
		</products>
		<skillRequirements>
			<Crafting>8</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
	</RecipeDef>
	
