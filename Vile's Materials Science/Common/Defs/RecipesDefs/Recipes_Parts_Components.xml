<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <RecipeDef>
		<defName>MakeSewingKits</defName>
		<label>craft sewing kits</label>
		<description>A sewing kit with needle, thread, buttons, grommets, metal components etc. Used for making clothing.</description>
		<jobString>Crafting mending kit.</jobString>
		<workAmount>500</workAmount>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Smith</effectWorking>
		<soundWorking>Recipe_Smith</soundWorking>
		<recipeUsers>
			<li>FabricationBench</li>
			<li>AdvToolBench</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentMedieval</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>Textiles</li>
					</categories>
				</filter>
				<count>15</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>SLDBar</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>BTextiles</li>
				<li>HTextiles</li>
				<li>SLDBar</li>
			</categories>
		</fixedIngredientFilter>
		<products>
			<SewingKit>10</SewingKit>
		</products>
		<skillRequirements>
			<Crafting>3</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
		<researchPrerequisite>Apparel_A2</researchPrerequisite>
	</RecipeDef>
    
